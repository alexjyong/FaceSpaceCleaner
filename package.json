{
  "name": "FaceSpace-Cleaner",
  "version": "1.1.0",
  "description": "Electron wrapper around fb-delete by spieglt. ",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/FaceSpaceClear.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon_32x32@2x.png --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"FaceSpace-Cleaner\"",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon_32x32@2x.png --prune=true --out=release-builds"
  },
  "author": "Alex Yong",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "bootstrap-select": "file:bootstrap-select",
    "jquery": "^3.2.1",
    "popper.js": "^1.12.9",
    "puppeteer": "^1.18.1"
  },
  "repository": "https://github.com/alexjyong/facespace-cleaner",
  "devDependencies": {
    "command-line-args": "^5.0.2",
    "electron": "^3.0.3",
    "electron-packager": "^13.1.1"
  }
}
